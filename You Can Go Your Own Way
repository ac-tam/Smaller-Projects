T = int(input())

for c in range(1, T+1):
    N = int(input())
    lydia = input()
    path = lydia
    for char in range(len(path)):
        if path[char] == 'S':
            path = path[:char] + "E" + path[char+1:]
        else:
            path = path[:char] + "S" + path[char+1:]
    print("Case #" + str(c) + ": " + str(path))
