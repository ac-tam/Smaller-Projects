#Problem at https://codingcompetitions.withgoogle.com/codejam/round/0000000000051705/0000000000088231

T = int(input())
for c in range(1, T+1):
    N = int(input())
    A, B = 0, 0
    le = len(str(N))
    for i in range(le):
        if str(N)[i] == "4":
            A += (10**(le-i-1))
    B = N - A
    print("Case #" + str(c) + ": " + str(A) + " " + str(B))
